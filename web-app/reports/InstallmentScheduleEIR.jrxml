	<parameter name="id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select 
	(select param_value from institution where id = '1') as "BANK NAME",--
	I.name as "BRANCH NAME",
	C.account_no as "ACCOUNT NUMBER",
	D.name3 as "LNAME", D.name1 as "FNAME", D.customer_id as "ID", C.opening_date as "DATE OPEN", C.maturity_date as "MATURITY DATE", C.pn_no as "PN #",
	E.address1 as "BARANGAY", G.description as "LOAN TYPE", H.description as "LOAN SECURITY",
	E.address2 as "TOWN", 
	E.address3 as "POSTAL CODE", 
		C.granted_amount as "LOAN AMOUNT",
		case when C.num_installments = 1 then null else A.total_installment_amount end as "MONTHLY INSTALLMENT",
		C.num_installments as "NO. OF MONTLHY INSTALLMENTS",
		C.monthly_interest_rate as "CONTRACTUAL RATE (MONTHLY)",
		
			case when (select L.amount from loan_deduction L 
			inner join loan_loan_deduction LL on LL.loan_deduction_id = L.id
			inner join loan LLL on LLL.id = LL.loan_loan_deductions_id
			where LLL.id = C.id and L.scheme_id = 1) is null then 0 else 
			(select L.amount from loan_deduction L 
			inner join loan_loan_deduction LL on LL.loan_deduction_id = L.id
			inner join loan LLL on LLL.id = LL.loan_loan_deductions_id
			where LLL.id = C.id and L.scheme_id = 1)  end as "SERVICE CHARGE",

			case when (select M.amount from loan_deduction M 
			inner join loan_loan_deduction MM on MM.loan_deduction_id = M.id
			inner join loan MMM on MMM.id = MM.loan_loan_deductions_id
			where MMM.id = C.id and M.scheme_id = 2) is null then 0 else 
			(select M.amount from loan_deduction M 
			inner join loan_loan_deduction MM on MM.loan_deduction_id = M.id
			inner join loan MMM on MMM.id = MM.loan_loan_deductions_id
			where MMM.id = C.id and M.scheme_id = 2) end as "SERVICE CHG. W/O DST AMT.",

			case when (select N.amount from loan_deduction N
			inner join loan_loan_deduction NN on NN.loan_deduction_id = N.id
			inner join loan NNN on NNN.id = NN.loan_loan_deductions_id
			where NNN.id = C.id and N.scheme_id = 4) is null then 0 else 
			(select N.amount from loan_deduction N
			inner join loan_loan_deduction NN on NN.loan_deduction_id = N.id
			inner join loan NNN on NNN.id = NN.loan_loan_deductions_id
			where NNN.id = C.id and N.scheme_id = 4) end as "NOTARIAL FEE",

			case when (select O.amount from loan_deduction O
			inner join loan_loan_deduction OO on OO.loan_deduction_id = O.id
			inner join loan OOO on OOO.id = OO.loan_loan_deductions_id
			where OOO.id = C.id and O.scheme_id = 6) is null then 0 else 
			(select O.amount from loan_deduction O
			inner join loan_loan_deduction OO on OO.loan_deduction_id = O.id
			inner join loan OOO on OOO.id = OO.loan_loan_deductions_id
			where OOO.id = C.id and O.scheme_id = 6) end as "INSURANCE",
			
			A.installment_date as "DUE DATE", 
			A.sequence_no as "INSTALLMENT PERIOD", 
			case when C.granted_amount is null then 0 else C.granted_amount end as "GROSS LOAN",
			A.principal_installment_amount as "PRINCIPAL",
			A.interest_installment_amount as "INTEREST",  
			C.total_net_proceeds as "LOAN PROCEEDS",
			A.total_installment_amount * (-1) as "CASH FLOWS",
				--Additional loan deduction to Cash flow
				
				case when (select P.amount from loan_deduction P
				inner join loan_loan_deduction PP on PP.loan_deduction_id = P.id
				inner join loan PPP on PPP.id = PP.loan_loan_deductions_id
				where PPP.id = C.id and P.scheme_id = 3) is null then 0 else 
				(select P.amount from loan_deduction P
				inner join loan_loan_deduction PP on PP.loan_deduction_id = P.id
				inner join loan PPP on PPP.id = PP.loan_loan_deductions_id
				where PPP.id = C.id and P.scheme_id = 3) end as "CI APPRAISAL FEE",

				case when (select Q.amount from loan_deduction Q
				inner join loan_loan_deduction QQ on QQ.loan_deduction_id = Q.id
				inner join loan QQQ on QQQ.id = QQ.loan_loan_deductions_id
				where QQQ.id = C.id and Q.scheme_id = 5) is null then 0 else 
				(select Q.amount from loan_deduction Q
				inner join loan_loan_deduction QQ on QQ.loan_deduction_id = Q.id
				inner join loan QQQ on QQQ.id = QQ.loan_loan_deductions_id
				where QQQ.id = C.id and Q.scheme_id = 5) end as "REM ANNOTATION FEE", 

				case when (select R.amount from loan_deduction R
				inner join loan_loan_deduction RR on RR.loan_deduction_id = R.id
				inner join loan RRR on RRR.id = RR.loan_loan_deductions_id
				where RRR.id = C.id and R.scheme_id = 7) is null then 0 else 
				(select R.amount from loan_deduction R
				inner join loan_loan_deduction RR on RR.loan_deduction_id = R.id
				inner join loan RRR on RRR.id = RR.loan_loan_deductions_id
				where RRR.id = C.id and R.scheme_id = 7) end as "PRINCIPAL BALANCE",

				case when (select S.amount from loan_deduction S
				inner join loan_loan_deduction SS on SS.loan_deduction_id = S.id
				inner join loan SSS on SSS.id = SS.loan_loan_deductions_id
				where SSS.id = C.id and S.scheme_id = 8) is null then 0 else 
				(select S.amount from loan_deduction S
				inner join loan_loan_deduction SS on SS.loan_deduction_id = S.id
				inner join loan SSS on SSS.id = SS.loan_loan_deductions_id
				where SSS.id = C.id and S.scheme_id = 8) end as "INTEREST BALANCE",

				case when (select T.amount from loan_deduction T
				inner join loan_loan_deduction TT on TT.loan_deduction_id = T.id
				inner join loan TTT on TTT.id = TT.loan_loan_deductions_id
				where TTT.id = C.id and T.scheme_id = 9) is null then 0 else 
				(select T.amount from loan_deduction T
				inner join loan_loan_deduction TT on TT.loan_deduction_id = T.id
				inner join loan TTT on TTT.id = TT.loan_loan_deductions_id
				where TTT.id = C.id and T.scheme_id = 9) end as "PENALTY BALANCE",

				case when (select U.amount from loan_deduction U
				inner join loan_loan_deduction UU on UU.loan_deduction_id = U.id
				inner join loan UUU on UUU.id = UU.loan_loan_deductions_id
				where UUU.id = C.id and U.scheme_id = 10) is null then 0 else 
				(select U.amount from loan_deduction U
				inner join loan_loan_deduction UU on UU.loan_deduction_id = U.id
				inner join loan UUU on UUU.id = UU.loan_loan_deductions_id
				where UUU.id = C.id and U.scheme_id = 10) end as "ADVANCES", 

				case when (select V.amount from loan_deduction V
				inner join loan_loan_deduction VV on VV.loan_deduction_id = V.id
				inner join loan VVV on VVV.id = VV.loan_loan_deductions_id
				where VVV.id = C.id and V.scheme_id = 11) is null then 0 else 
				(select V.amount from loan_deduction V
				inner join loan_loan_deduction VV on VV.loan_deduction_id = V.id
				inner join loan VVV on VVV.id = VV.loan_loan_deductions_id
				where VVV.id = C.id and V.scheme_id = 11) end as "OTHERS",
				
		(select C.granted_amount - sum(X.principal_installment_amount) from loan_installment X
		inner join loan_loan_installment XX on XX.loan_installment_id = X.id
		inner join loan XXX on  XXX.id = XX.loan_loan_installments_id
		where XXX.id = C.id and X.installment_date <= A.installment_date) as "BALANCE O/S",
	C.effective_interest_rate as "EFFECTIVE ANNUAL INTEREST RATE",
	C.monthly_interest_rate as "EFFECTIVE MONTHLY INTEREST RATE (MIR)"
from loan_installment A
	inner join loan_loan_installment B on B.loan_installment_id = A.id
	inner join loan C on C.id = B.loan_loan_installments_id
	inner join branch I on C.branch_id = I.id
	inner join customer D on C.customer_id = D.id
	left outer join Address E on E.customer_id = D.id and is_primary = TRUE
	inner join branch F on C.branch_id = F.id
	inner join loan_type G on C.loan_type_id = G.id
	inner join loan_security H on C.loan_security_id = H.id
where C.id = $P{id}
group by I.name, C.account_no, D.name3, D.name1, D.customer_id, C.opening_date, C.maturity_date, C.pn_no, E.address1, E.address2, E.address3,
A.total_installment_amount, C.num_installments, C.monthly_interest_rate, G.description, H.description, C.granted_amount, C.effective_interest_rate, 
C.monthly_interest_rate, A.installment_date, A.sequence_no, A.principal_installment_amount, A.interest_installment_amount, C.id,C.num_installments
order by A.sequence_no]]>
	</queryString>
